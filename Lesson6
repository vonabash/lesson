class Animal {
        public String name;
    public int run;
 
    public Animal() {
    }
 
    public Animal(String name) {
        this.name = name;
    }
 
    public void animalInfo() {
        System.out.println();
 
    }
}

class Cat extends Animal {
 
   protected boolean swim;
 
    public Cat(String name, int run, boolean swim) {
        this.name = name;
        this.run = run;
        this.sweem = swim;
        }

    public void catInfo() {
        System.out.println("CatName: " + name + " /RunLimit: " + run+ " meters/" + " /Swim:" + swim + ");
    }
}
 
class Dog extends Animal {
    public int swim
 
        public Dog(String name, int run, int sweem,) {
            this.name = name;
            this.run = run;
            this.sweem = swim;
        }
        public void dogInfo() {
            System.out.println("DogName: " + name  + " /RunLimit: " + run+ " meters/" + " /Swim:" + swim + ");
        }
    }
 
public class Main {
    public static void main(String[] args) {
 
        Animal animal = new Animal("CatsCategory");
        Cat cat = new Cat("Misha", 200, false);
        animal.animalInfo();
        cat.animalInfo();
        cat.catInfo();
 
        Animal animal = new Animal("Dogs");
        Dog dog = new Dog("Nessy", 500, 10);
        danimal.animalInfo();
        dog.animalInfo();
        dog.dogInfo();
 
   }
}
